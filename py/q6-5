A = [-5, 9, -3, -2, -9, -2, 2, 11, -1, -4, -10] 
B = [-4, -4, -2, -9, -2, 8, 3, 10, -2, -3, -9]
A2 = [-5, 9, -3, -2, -9, -2, 2, 11, -1, -4, -10] 
B2 = [-4, -4, -2, -9, -2, 8, 3, 10, -2, -3, -9]


for c1, i in enumerate(A2):
    for c2, j in enumerate(B2):
        if i - j < 2:
            print(i, j)
            A2 = A[:c1] + A[c1+1:]
            B2 = B[:c2] + B[c2+1:]
            print(A2, B2)
            break

